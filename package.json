    "dotenv": "^10.0.0",
    "handlebars": "^4.7.7",
    "handlebars-helpers": "^0.10.0",
    "handlebars-loader": "^1.7.1",
    "highlight.js": "^11.3.1",
    "laravel-mix": "^6.0.19",
    "laravel-mix-alias": "^1.0.3",
    "laravel-mix-polyfill": "^2.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.foreach": "^4.5.0",
